
# Variables for ldap_server and ldap_client roles
#
# Edit to set your LDAP domain and passwords
ldap_domain: dc=spp,dc=zapto,dc=org
ldap_domain_short: spp
#ldap_domain_password: SET_YOUR_LDAP_DOMAIN_PASSWORD
#ldap_root_password: SET_YOUR_LDAP_ROOT_PASSWORD

# if ldap_domain_password or ldap_root_password is not defined,
# BIOS UUID of a ldap server is used for the password.



# Create and Edit machine-configs/files/ldap_users.csv and
# machine-configs/files/ldap_groups.csv.
# Please see machine-configs/files/README.md
ldap_users_csv: files/ldap_users.csv
ldap_groups_csv: files/ldap_groups.csv
